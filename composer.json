{
    "name": "ahmdadl/deployer-cli",
    "description": "Deployer for custom nodejs deployer.",
    "keywords": [
        "deployer",
        "laravel",
        "laravel zero",
        "console",
        "cli"
    ],
    "homepage": "https://github.com/ahmdadl/deployer-cli",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ahmdadl/deployer-cli/issues",
        "source": "https://github.com/ahmdadl/deployer-cli"
    },
    "authors": [
        {
            "name": "Ahmed Adel",
            "email": "ahmdadl.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "illuminate/http": "^11.5",
        "laravel-zero/framework": "^11.0.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.15.2",
        "mockery/mockery": "^1.6.11",
        "pestphp/pest": "^2.34.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "deployer"
    ]
}
